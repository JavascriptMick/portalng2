<!DOCTYPE html>
<html ng-app="portalng">
<head>
  <meta charset="utf8">
  <base href="/">
  <title>Portal</title>
  <link rel="stylesheet" href="/css/app.css">
</head>
<body>
  <div ng-controller="AppCtrl">
    <portalng portal-url="">
      <h2>Portal Home</h2>
      <h3>Available Applets</h3>
      <ul>
      <% applets.forEach(function(applet){%>
        <li><a href="<%= applet.url %>"><%= applet.name %></a></li>
      <% }); %>
      </ul>
    </portalng>
  </div>
  <script src="js/lib/angular/angular.js"></script>
  <script src="js/app.js"></script>
  <script src="js/services.js"></script>
  <script src="js/controllers.js"></script>
  <script src="js/filters.js"></script>
  <!-- bring in the portal components -->
  <script src="js/lib/portalng/portalng.0.0.1.js"></script>

</body>
</html>